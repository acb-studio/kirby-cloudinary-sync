{
  "name": "acb-studio/kirby-cloudinary-sync",
  "version": "1.1.0",
  "type": "kirby-plugin",
  "description": "Kirby -> Cloudinary media sync plugin",
  "license": "MIT",
  "authors": [
    {
      "name": "ACB Studio GmbH",
      "homepage": "https://acb.studio/"
    },
    {
      "name": "Jonathan Reisdorf",
      "role": "Engineering"
    }
  ],
  "config": {
    "optimize-autoloader": true,
